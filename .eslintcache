    <FormGroup className=\"mt-3\">\r\n            <Label>Select Genres:</Label>\r\n            <div>\r\n                {getGenresCheckboxes()}\r\n            </div>\r\n        </FormGroup>\r\n       \r\n    );\r\n}\r\n\r\nexport default SelectGenres;","C:\\Users\\Veni\\Desktop\\webbapp\\shop-app\\src\\redux\\actions.js",[],"C:\\Users\\Veni\\Desktop\\webbapp\\shop-app\\src\\redux\\action-types.js",[],"C:\\Users\\Veni\\Desktop\\webbapp\\shop-app\\src\\redux\\reducers.js",[],"C:\\Users\\Veni\\Desktop\\webbapp\\shop-app\\src\\Pages\\Orders.js",[],"C:\\Users\\Veni\\Desktop\\webbapp\\shop-app\\src\\Pages\\TVSeries.js",["132"],"C:\\Users\\Veni\\Desktop\\webbapp\\shop-app\\src\\tvseries\\index.js",[],"C:\\Users\\Veni\\Desktop\\webbapp\\shop-app\\src\\tvseries\\components\\SeriesList.js",[],"C:\\Users\\Veni\\Desktop\\webbapp\\shop-app\\src\\tvseries\\components\\loader.js",[],"C:\\Users\\Veni\\Desktop\\webbapp\\shop-app\\src\\tvseries\\components\\intro.js",[],{"ruleId":"133","replacedBy":"134"},{"ruleId":"135","replacedBy":"136"},{"ruleId":"137","severity":1,"message":"138","line":32,"column":7,"nodeType":"139","endLine":32,"endColumn":9,"suggestions":"140"},{"ruleId":"141","severity":1,"message":"142","line":5,"column":3,"nodeType":"143","messageId":"144","endLine":5,"endColumn":13},{"ruleId":"137","severity":1,"message":"145","line":25,"column":8,"nodeType":"139","endLine":25,"endColumn":10,"suggestions":"146"},{"ruleId":"137","severity":1,"message":"147","line":17,"column":8,"nodeType":"139","endLine":17,"endColumn":10,"suggestions":"148"},{"ruleId":"141","severity":1,"message":"149","line":6,"column":24,"nodeType":"143","messageId":"144","endLine":6,"endColumn":31},"no-native-reassign",["150"],"no-negated-in-lhs",["151"],"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'dispatch' and 'id'. Either include them or remove the dependency array.","ArrayExpression",["152"],"no-unused-vars","'NavbarText' is defined but never used.","Identifier","u